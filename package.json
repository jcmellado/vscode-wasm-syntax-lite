{
  "name": "vscode-wasm-syntax-lite",
  "version": "0.2.0",
  "author": {
    "name": "Juan Mellado"
  },
  "homepage": "https://github.com/jcmellado/vscode-wasm-syntax-lite",
  "repository": {
    "type": "git",
    "url": "https://github.com/jcmellado/vscode-wasm-syntax-lite.git"
  },
  "bugs": {
    "url": "https://github.com/jcmellado/vscode-wasm-syntax-lite/issues"
  },
  "publisher": "jcmellado",
  "engines": {
    "vscode": "^1.23.0"
  },
  "license": "MIT",
  "displayName": "WebAssembly Syntax (Lite)",
  "icon": "images/webassembly.png",
  "description": "WebAssembly Syntax for Visual Studio Code",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "webassembly",
    "wasm",
    "javascript"
  ],
  "contributes": {
    "languages": [
      {
        "id": "webassembly",
        "extensions": [
          ".wat"
        ],
        "aliases": [
          "WebAssembly",
          "webassembly",
          "WebAssembly Text Format",
          "wat"
        ],
        "configuration": "./wat.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "webassembly",
        "scopeName": "source.wat",
        "path": "./syntaxes/wat.tmLanguage.json"
      }
    ]
  }
}
